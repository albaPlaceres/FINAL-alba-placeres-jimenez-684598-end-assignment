<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="230.0" prefWidth="344.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalalbaplaceresjimenez684598endassignment.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="loginText" text="Login" textFill="#9e9e9e">
        <font>
            <Font name="Arial" size="31.0" />
        </font></Label>
   <TextField fx:id="usernameText" promptText="Username" />
   <PasswordField fx:id="passwordText" promptText="Password" />
    <Button alignment="CENTER" onAction="#onLoginButtonClick" prefHeight="26.0" prefWidth="135.0" style="-fx-background-color: #3692ff;" text="Login" textFill="WHITE" />
   <Label fx:id="message" />
</VBox>
